{
  "name": "muinaismuistot",
  "version": "1.14.0",
  "description": "Tutki kartalla Museoviraston ylläpitämien kulttuuriperintörekistereiden paikkatietoa.",
  "scripts": {
    "build:prod": "NODE_ENV=production webpack --mode=production --progress",
    "build:dev": "webpack --mode=development --progress --devtool source-map",
    "dev": "webpack serve --mode=development --server-type https --host 0.0.0.0 --port 8091 --allowed-hosts all --progress --hot --open",
    "prod": "NODE_ENV=production webpack serve --mode=production --server-type https --host 0.0.0.0 --port 8091 --allowed-hosts all --progress --hot --open",
    "profile-size": "NODE_ENV=production SHOW_BUNDLE_ANALYZER=true webpack --progress",
    "addDataToMaisemanMuisti": "node scripts/addDataToMaisemanMuisti.js"
  },
  "author": "Antti Kekki",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/anttikekki/muinaismuistot.git"
  },
  "dependencies": {
    "bootstrap": "3.4.1",
    "i18next": "22.4.9",
    "ol": "7.2.2",
    "proj4": "2.8.1",
    "query-string": "7.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "12.1.4",
    "react-redux": "8.0.5",
    "redux": "4.2.0",
    "@redux-devtools/extension": "3.2.5",
    "redux-observable": "2.0.0",
    "regexify-string": "1.0.19",
    "rxjs": "7.8.0",
    "tslib": "2.4.1"
  },
  "devDependencies": {
    "@types/proj4": "2.5.2",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.3",
    "html-webpack-plugin": "5.5.0",
    "prettier": "2.8.3",
    "style-loader": "3.3.1",
    "ts-loader": "9.4.2",
    "typescript": "4.9.4",
    "url-loader": "4.1.1",
    "webpack": "5.75.0",
    "webpack-bundle-analyzer": "4.7.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1"
  },
  "engines": {
    "node": ">=18"
  }
}
